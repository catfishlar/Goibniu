<h1>Listing schema_fields</h1>

<table>
  <thead>
    <tr>
      <th>Schema table</th>
      <th>Name</th>
      <th>Field type</th>
      <th>Type params</th>
      <th>Nullable</th>
      <th>Default</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schema_fields.each do |schema_field| %>
      <tr>
        <td><%= schema_field.schema_table_id %></td>
        <td><%= schema_field.name %></td>
        <td><%= schema_field.field_type %></td>
        <td><%= schema_field.type_params %></td>
        <td><%= schema_field.nullable %></td>
        <td><%= schema_field.default %></td>
        <td><%= link_to 'Show', schema_field %></td>
        <td><%= link_to 'Edit', edit_schema_field_path(schema_field) %></td>
        <td><%= link_to 'Destroy', schema_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schema field', new_schema_field_path %>
