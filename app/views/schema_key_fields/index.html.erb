<h1>Listing schema_key_fields</h1>

<table>
  <thead>
    <tr>
      <th>Schema key</th>
      <th>Name</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schema_key_fields.each do |schema_key_field| %>
      <tr>
        <td><%= schema_key_field.schema_key_id %></td>
        <td><%= schema_key_field.name %></td>
        <td><%= schema_key_field.order %></td>
        <td><%= link_to 'Show', schema_key_field %></td>
        <td><%= link_to 'Edit', edit_schema_key_field_path(schema_key_field) %></td>
        <td><%= link_to 'Destroy', schema_key_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schema key field', new_schema_key_field_path %>
