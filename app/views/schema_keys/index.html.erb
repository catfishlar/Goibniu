<h1>Listing schema_keys</h1>

<table>
  <thead>
    <tr>
      <th>Schema table</th>
      <th>Name</th>
      <th>Primary</th>
      <th>Unique</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schema_keys.each do |schema_key| %>
      <tr>
        <td><%= schema_key.schema_table_id %></td>
        <td><%= schema_key.name %></td>
        <td><%= schema_key.primary %></td>
        <td><%= schema_key.unique %></td>
        <td><%= link_to 'Show', schema_key %></td>
        <td><%= link_to 'Edit', edit_schema_key_path(schema_key) %></td>
        <td><%= link_to 'Destroy', schema_key, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schema key', new_schema_key_path %>
