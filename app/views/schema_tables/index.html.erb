<h1>Listing schema_tables</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Schema version</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schema_tables.each do |schema_table| %>
      <tr>
        <td><%= schema_table.name %></td>
        <td><%= schema_table.schema_version_id %></td>
        <td><%= link_to 'Show', schema_table %></td>
        <td><%= link_to 'Edit', edit_schema_table_path(schema_table) %></td>
        <td><%= link_to 'Destroy', schema_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schema table', new_schema_table_path %>
